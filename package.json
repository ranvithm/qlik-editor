{
  "name": "qlik-script-editor",
  "version": "1.0.1",
  "description": "A React Monaco Editor component for Qlik Sense scripts with syntax highlighting and autocomplete",
  "type": "module",
  "main": "./dist/qlik-script-editor.cjs.js",
  "module": "./dist/qlik-script-editor.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/qlik-script-editor.es.js",
      "require": "./dist/qlik-script-editor.cjs.js"
    },
    "./styles": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "qlik",
    "qlik-sense",
    "monaco-editor",
    "react",
    "typescript",
    "syntax-highlighting",
    "autocomplete",
    "editor"
  ],
  "author": {
    "name": "Ranjith M",
    "url": "https://ranjithm.in/",
    "email": "hello@ranjithm.in"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranvithm/qlik-script-editor.git",
    "directory": "packages/qlik-script-editor"
  },
  "homepage": "https://github.com/ranvithm/qlik-script-editor#readme",
  "bugs": {
    "url": "https://github.com/ranvithm/qlik-script-editor/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && tsc --project tsconfig.build.json",
    "lint": "eslint . --ext ts,tsx",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "monaco-editor": ">=0.44.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.532.0",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.6",
    "tw-animate-css": "^1.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^9.30.1",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4"
  }
}
