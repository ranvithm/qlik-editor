{
  "name": "@ranjith/qlik-script-editor",
  "version": "1.0.0",
  "description": "A React Monaco Editor component for Qlik Sense scripts with syntax highlighting and autocomplete",
  "type": "module",
  "main": "./lib/qlik-editor.cjs.js",
  "module": "./lib/qlik-editor.es.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/qlik-editor.es.js",
      "require": "./lib/qlik-editor.cjs.js"
    },
    "./styles": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    }
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "qlik",
    "qlik-sense",
    "monaco-editor",
    "react",
    "typescript",
    "syntax-highlighting",
    "autocomplete",
    "editor"
  ],
  "author": "Ranjith",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ranjith/qlik-script-editor.git"
  },
  "homepage": "https://github.com/ranjith/qlik-script-editor#readme",
  "bugs": {
    "url": "https://github.com/ranjith/qlik-script-editor/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx tsc src/index.ts --declaration --emitDeclarationOnly --outDir lib --moduleResolution bundler --target ES2022 --module ESNext --jsx react-jsx --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck",
    "build:lib": "vite build && npx tsc src/index.ts --declaration --emitDeclarationOnly --outDir lib --moduleResolution bundler --target ES2022 --module ESNext --jsx react-jsx --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "monaco-editor": ">=0.44.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.532.0",
    "monaco-editor": "^0.52.2",
    "next-themes": "^0.4.6",
    "sonner": "^2.0.6",
    "tw-animate-css": "^1.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
